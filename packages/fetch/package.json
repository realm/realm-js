{
  "name": "@realm/fetch",
  "version": "0.1.0",
  "module": "true",
  "private": true,
  "scripts": {
    "bundle": "wireit"
  },
  "wireit": {
    "bundle": {
      "command": "tsc --build",
      "files": [
        "src",
        "types.d.ts",
        "tsconfig.json",
        "tsconfig.*.json"
      ],
      "output": [
        "dist"
      ]
    }
  },
  "exports": {
