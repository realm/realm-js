{
  "name": "@realm/bindgen",
  "version": "0.1.0",
  "private": true,
  "description": "Binding generator exposing Realm Core to the SDK",
  "bin": {
    "realm-bindgen": "./realm-bindgen.ts"
  },
  "type": "module",
  "main": "./index.ts",
  "exports": "./index.ts",
  "types": "./generated/ts/native.d.ts",
  "scripts": {
    "start": "./realm-bindgen.ts",
    "test": "mocha",
    "generate:spec-schema": "typescript-json-schema src/spec/relaxed-model.ts RelaxedSpec --out generated/spec.schema.json --required",
    "generate:type-parser-diagram": "tsm --no-warnings src/spec/type-parser-diagram.ts generated/type-parser.html",
    "generate:typescript": "./realm-bindgen.ts --spec spec.yml --template typescript --output generated/ts",
    "generate:node": "./realm-bindgen.ts --spec spec.yml --template node --output generated/node",
    "compile:node": "cmake-js compile --debug"
  },
  "dependencies": {
    "@types/node": "^12.20.55",
    "ajv": "^8.11.0",
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "chevrotain": "^10.1.2",
    "commander": "^9.3.0",
    "debug": "^4.3.4",
    "tsm": "^2.2.1",
    "typescript-json-schema": "^0.53.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "cmake-js": "^6.1.0",
    "mocha": "^10.0.0"
  }
}