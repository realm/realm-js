{
  "env": {
    "commonjs": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "forOf": false,
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": 0,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-direct-mutation-state": 1,
    "react/prefer-es6-class": 1,
    "react/react-in-jsx-scope": 2
  }
}
