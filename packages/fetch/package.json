{
  "name": "@realm/fetch",
  "module": "true",
  "private": true,
  "scripts": {
    "bundle": "wireit"
  },
  "wireit": {
    "bundle": {
      "command": "tsc --build",
      "files": [
        "src",
        "types.d.ts",
        "tsconfig.json",
        "tsconfig.*.json"
      ],
      "output": [
        "dist"
      ]
    }
  },
  "exports": {
    ".": {
      "node": "./dist/node/node.js",
      "react-native": "./dist/react-native/react-native.js",
      "browser": "./dist/browser/browser.js",
      "types": "./dist/types/types.d.ts"
    }
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@tsconfig/recommended": "^1.0.3"
  }
}