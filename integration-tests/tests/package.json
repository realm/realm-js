{
  "name": "realm-integration-tests",
  "version": "0.1.0",
  "description": "A set of tests that can run in different environments",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "start": "concurrently npm:app-importer npm:mocha",
    "app-importer": "realm-app-importer serve ../realm-apps",
    "mocha": "ts-mocha --project ./tsconfig.json --require src/utils/inject-node-globals.ts --watch-extensions ts,js --watch src/index.ts",
    "lint": "tslint -p .",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "bson": "^4.2.3",
    "mocha": "^5.2.0",
    "realm": "*"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "concurrently": "^6.0.2",
    "fs-extra": "^7.0.1",
    "mocha": "^5.2.0",
    "node-fetch": "^2.6.1",
    "realm": "*",
    "realm-app-importer": "*",
    "ts-mocha": "^6.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "chai": "^4.2.0"
  },
  "files": [
    "/dist"
  ]
}
