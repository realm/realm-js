{
  "env": {
    "jasmine": true
  },
  "plugins": ["jasmine"],
  "extends": "plugin:jasmine/recommended",
  "globals": {
    "ArrayBuffer": false,
    "DataView": false,
    "Float32Array": false,
    "Float64Array": false,
    "Int16Array": false,
    "Int32Array": false,
    "Int8Array": false,
    "Uint16Array": false,
    "Uint32Array": false,
    "Uint8Array": false,
    "Uint8ClampedArray": false
  },
  "rules": {
    "no-invalid-this": "off",
    "no-magic-numbers": "off",
    "jasmine/no-spec-dupes": ["error", "branch"],
    "jasmine/no-suite-dupes": ["error", "branch"],
    "no-console": "off"
  }
}
