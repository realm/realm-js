{
  "name": "@realm/mocha-reporter",
  "version": "0.0.1",
  "description": "A custom reporter for mocha",
  "main": "./dist/index.js",
  "scripts": {
    "bundle": "wireit"
  },
  "wireit": {
    "bundle": {
      "command": "tsc",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "dist/**/*.js",
        "dist/**/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "MongoDB",
    "email": "help@realm.io",
    "url": "https://www.mongodb.com/docs/realm/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/realm/realm-js.git"
  },
  "bugs": {
    "url": "https://github.com/realm/realm-js/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "mocha": "^8",
    "mocha-github-actions-reporter": "^0.3.1"
  },
  "devDependencies": {
    "@types/mocha": "^8"
  }
}
