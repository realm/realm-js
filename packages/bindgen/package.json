{
  "name": "@realm/bindgen",
  "version": "0.1.0",
  "private": true,
  "description": "Binding generator exposing Realm Core to the SDK",
  "bin": {
    "realm-bindgen": "./realm-bindgen.ts"
  },
  "type": "module",
  "main": "./realm-bindgen.ts",
  "scripts": {
    "start": "./realm-bindgen.ts",
    "test": "mocha",
    "generate:spec-schema": "typescript-json-schema src/spec/relaxed-model.ts RelaxedSpec --out generated/spec.schema.json --required",
    "compile:node": "cmake-js compile --debug"
  },
  "dependencies": {
    "@types/node": "^18.11.9",
    "ajv": "^8.11.0",
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "chevrotain": "^10.4.0",
    "commander": "^9.4.1",
    "debug": "^4.3.4",
    "typescript-json-schema": "^0.55.0",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^10.0.0",
    "chai": "^4.3.6",
    "cmake-js": "^7.0.0",
    "mocha": "^10.1.0"
  }
}
