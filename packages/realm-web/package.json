{
  "name": "realm-web",
  "version": "0.1.0",
  "description": "Authenticate and communicate with the MongoDB Realm platform, from your web-browser",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.es.js",
  "types": "dist/bundle.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "prepare": "npm run update-types",
    "update-types": "ts-node scripts/update-types.ts",
    "generate-types": "tsc --project tsconfig.build.json --declaration --emitDeclarationOnly --declarationDir types/generated",
    "build": "npm run generate-types && rollup --config",
    "lint": "eslint --ext .js,.ts .",
    "test": "mocha 'src/**/*.test.ts'"
  },
  "files": [
    "dist",
    "types/realm",
    "realm-network-transport-0.1.0.tgz",
    "LICENSE"
  ],
  "keywords": [
    "realm",
    "browser"
  ],
  "author": {
    "name": "Realm",
    "email": "help@realm.io",
    "url": "https://realm.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/realm/realm-js.git"
  },
  "bugs": {
    "url": "https://github.com/realm/realm-js/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "bson": "^4.0.3"
  },
  "optionalDependencies": {
    "node-fetch": "^2.6.0",
    "abort-controller": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^4.1.1",
    "@types/bson": "^4.0.2",
    "@types/chai": "^4.2.9",
    "@types/fs-extra": "^8.1.0",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.6",
    "abort-controller": "^3.0.0",
    "chai": "^4.2.0",
    "eslint": "file:../../node_modules/eslint",
    "mocha": "^5.2.0",
    "node-fetch": "^2.6.0",
    "realm-network-transport": "^0.1.0",
    "rollup": "^2.6.1",
    "rollup-plugin-dts": "^1.4.0",
    "ts-node": "^8.8.2",
    "typescript": "file:../../node_modules/typescript"
  }
}
