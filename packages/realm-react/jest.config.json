{
  "verbose": true,
  "preset": "react-native",
  "roots": [
    "<rootDir>/src"
  ],
  "setupFilesAfterEnv": [
    "@testing-library/react-native/extend-expect",
    "<rootDir>/src/__tests__/setup.ts"
  ],
  "transform": {
    "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
  },
  "testRegex": "(/__tests__/.*.(test|spec)).(jsx?|tsx?|js?|ts?)$",
  "testTimeout": 30000
}
