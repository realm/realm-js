{
  "name": "realm-web-integration-tests",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "start": "wireit",
    "lint": "eslint --ext .js,.ts .",
    "test": "wireit",
    "test:github": "wireit"
  },
  "wireit": {
    "start": {
      "command": "mocha --file harness/node.ts src/**/*.test.ts --watch",
      "dependencies": [
        "../realm-web:bundle"
      ]
    },
    "test": {
      "command": "tsx harness/dev.ts",
      "dependencies": [
        "../realm-web:bundle"
      ]
    },
    "test:github": {
      "command": "tsx harness/github.ts",
      "dependencies": [
        "../realm-web:bundle"
      ]
    }
  },
  "dependencies": {
    "@realm/app-importer": "^0.1.0",
    "chai": "4.3.6",
    "js-base64": "^3.7.6",
    "jwt-encode": "^1.0.1",
    "mocha-remote-cli": "^1.12.2",
    "mocha-remote-server": "^1.12.2",
    "puppeteer": "^22.4.1",
    "realm-web": "*",
    "webpack": "^5.91.0",
    "webpack-dev-server": "^5.0.4"
  },
  "peerDependencies": {
    "mocha-remote-client": "^1.12.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^8",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.17.0",
    "fs-extra": "^11.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mongodb-realm-cli": "^1.3.2",
    "source-map-loader": "^5.0.0",
    "ts-loader": "^9.5.1",
    "webpack-cli": "^5.1.4"
  }
}
