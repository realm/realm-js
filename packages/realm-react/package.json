{
  "name": "@realm/react",
  "version": "0.4.3",
  "description": "React specific hooks and implementation helpers for Realm",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.tsx",
  "scripts": {
    "bundle": "wireit",
    "start": "npm run bundle -- --watch",
    "test": "wireit",
    "lint": "eslint --ext .tsx .",
    "docs": "wireit"
  },
  "wireit": {
    "bundle": {
      "command": "rollup --config",
      "dependencies": [
        "../realm:bundle",
        "../realm-common:bundle"
      ],
      "files": [
        "rollup.config.mjs",
        "src/**/*.ts",
        "src/**/*.tsx"
      ],
      "output": [
        "dist"
      ]
    },
    "test": {
      "command": "jest --forceExit",
      "dependencies": [
        "../realm:bundle",
        "../realm:build:node",
        "../realm-common:bundle"
      ]
    },
    "docs": {
      "command": "typedoc",
      "dependencies": [
        "bundle"
      ]
    }
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@realm/app-importer": "*",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@testing-library/jest-native": "^4.0.13",
    "@testing-library/react-native": "^12.1.1",
    "@types/jest": "^26.0.24",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.21",
    "babel-jest": "^29.4.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "fb-watchman": "^2.0.1",
    "jest": "^29.1.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.3.2",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-test-renderer": "18.2.0",
    "realm": "*",
    "rollup-plugin-dts": "^5.0.0",
    "ts-jest": "^29.0.5"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "realm": "^12.0.0 || ^12.0.0-rc || ^11.0.0-rc || ^11.0.0"
  },
  "optionalDependencies": {
    "@babel/runtime": ">=7",
    "react-native": ">=0.68"
  },
  "files": [
    "dist",
    "types/realm",
    "LICENSE",
    "lib",
    "package.json"
  ],
  "keywords": [
    "realm",
    "react",
    "hooks"
  ],
  "author": {
    "name": "Realm",
    "email": "help@realm.io",
    "url": "https://realm.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/realm/realm-js.git",
    "directory": "packages/realm-react"
  },
  "homepage": "https://github.com/realm/realm-js/tree/main/packages/realm-react#readme",
  "bugs": {
    "url": "https://github.com/realm/realm-js/issues"
  },
  "license": "Apache-2.0"
}
