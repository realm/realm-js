{
  "name": "realm-react-native-tests",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "npm run test/android && npm run test/ios",
    "test/android": "node harness/runner.js --platform=android",
    "test/ios": "node harness/runner.js --platform=ios",
    "preinstall": "test -d ../../tests/node_modules || (cd ../../tests; npm install --production)",
    "postinstall": "npm install ../../realm-*.tgz --no-save"
  },
  "dependencies": {
    "@realm-tests/tests": "file:../../tests",
    "mocha": "^5.2.0",
    "mocha-remote-client": "^0.8.1",
    "react": "16.4.1",
    "react-native": "0.56.0",
    "react-native-fs": "^2.13.3"
  },
  "devDependencies": {
    "babel-preset-react-native": "^5",
    "mocha-remote-server": "^0.8.1",
    "react-test-renderer": "16.4.1"
  }
}
