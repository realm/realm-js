{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"strict": true,
		"strictPropertyInitialization": true,
		"target": "es2022",
		"module": "es2022",
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"noResolve": true,
		"lib": [
			"es2019",
			"dom"
		]
	},
	"include": [
		"src/**/*.ts"
	],
}
