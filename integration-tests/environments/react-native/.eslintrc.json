{
  "extends": [
    "../../.eslintrc.json",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native",
    "flowtype"
  ],
  "env": {
    "react-native/react-native": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
