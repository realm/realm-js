{
  "verbose": true,
  "preset": "react-native",
  "resolver": "./custom-resolver.cjs",
  "roots": [
    "<rootDir>/src"
  ],
  "setupFilesAfterEnv": [
    "@testing-library/jest-native/extend-expect"
  ],
  "transform": {
    "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
  },
  "testRegex": "(/__tests__/.*.(test|spec)).(jsx?|tsx?|js?|ts?)$",
  "testTimeout": 30000
}