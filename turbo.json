{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "@realm/bindgen#generate:spec-schema": {
      "inputs": [
        "src/spec/model.ts",
        "src/spec/relaxed-model.ts"
      ],
      "outputs": [
        "generated/spec.schema.json"
      ]
    },
    "@realm/bindgen#build": {
      "inputs": [
        "src/",
        "generated/"
      ],
      "dependsOn": [
        "generate:spec-schema"
      ]
    },
    "@realm/binding#compile": {
      "inputs": [
        "../bindgen/src/"
      ],
      "outputs": [
        "build/Release/realm.node",
        "build/Debug/realm.node",
        "generated/"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "@realm/binding#build": {
      "inputs": [
        "binding/",
        "src/"
      ],
      "outputs": [
        "dist/"
      ],
      "dependsOn": [
        "compile"
      ]
    },
    "realm#test:types": {
      "inputs": [
        "type-tests/"
      ],
      "outputs": [],
      "dependsOn": [
        "build"
      ]
    },
    "realm#test": {
      "dependsOn": [
        "test:types"
      ]
    },
    "build": {
      "inputs": [
        "src/",
        "!src/tests/"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "inputs": [
        ".mocharc.json",
        "src/"
      ],
      "outputs": [],
      "dependsOn": [
        "build"
      ]
    },
  }
}