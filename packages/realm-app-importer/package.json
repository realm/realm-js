{
  "name": "@realm/app-importer",
  "version": "0.1.0",
  "description": "Wrapping the Stitch CLI to import an app from a template (w. secrets)",
  "type": "module",
  "types": "./dist/index.d.ts",
  "react-native": "./dist/bundle.react-native.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/bundle.mjs",
      "require": "./dist/bundle.cjs"
    }
  },
  "bin": {
    "realm-app-importer": "./realm-app-importer.ts"
  },
  "scripts": {
    "bundle": "wireit",
    "lint": "eslint --ext .js,.ts .",
    "start": "./realm-app-importer.ts"
  },
  "wireit": {
    "bundle": {
      "command": "rollup --config",
      "files": ["rollup.config.mjs", "src/**/*.ts", "src/**/tsconfig.json"],
      "output": ["dist/**/*.js", "dist/**/*.d.ts"],
      "dependencies": [
        "generate-types",
        "../realm-network-transport:bundle"
      ]
    },
    "generate-types": {
      "command": "tsc --project tsconfig.types.json --declaration --emitDeclarationOnly --declarationDir types/generated",
      "dependencies": [
        "../realm-network-transport:bundle"
      ],
      "files": ["src/**/*.ts"],
      "output": ["types/generated/**/*.d.ts"]
    }
  },
  "files": [
    "dist",
    "templates",
    "realm-app-importer"
  ],
  "author": {
    "name": "Realm",
    "email": "help@realm.io",
    "url": "https://realm.io"
  },
  "contributors": [
    "Kr√¶n Hansen <kraen.hansen@mongodb.com> (https://github.com/kraenhansen)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/realm/realm-js.git",
    "directory": "packages/realm-app-importer"
  },
  "bugs": {
    "url": "https://github.com/realm/realm-js/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@realm/network-transport": "*",
    "body-parser": "^1.20.1",
    "chalk": "^4.1.2",
    "deepmerge": "^4.2.2",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "node-fetch": "^2.6.9",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/body-parser": "^1.19.2",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.15.10",
    "@types/node-fetch": "^2.6.2",
    "@types/yargs": "^17.0.13",
    "rollup": "^3.2.5",
    "rollup-plugin-dts": "^5.0.0"
  }
}
